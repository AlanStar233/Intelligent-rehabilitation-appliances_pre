    241          void MT_UartProcessZAppData ( uint8 port, uint8 event )
    242          {
    243          
    244            osal_event_hdr_t  *msg_ptr;
    245            uint16 length = 0;
    246            uint16 rxBufLen  = Hal_UART_RxBufLen(MT_UART_DEFAULT_PORT);
    247          
    248            /*
    249               If maxZAppBufferLength is 0 or larger than current length
    250               the entire length of the current buffer is returned.
    251            */
    252            if ((MT_UartMaxZAppBufLen != 0) && (MT_UartMaxZAppBufLen <= rxBufLen))
    253            {
    254              length = MT_UartMaxZAppBufLen;
    255            }
    256            else
    257            {
    258              length = rxBufLen;
    259            }
    260          
    261            /* Verify events */
    262            if (event == HAL_UART_TX_FULL)
    263            {
    264              // Do something when TX if full
    265              return;
    266            }
    267          
    268            if (event & ( HAL_UART_RX_FULL | HAL_UART_RX_ABOUT_FULL | HAL_UART_RX_TIMEOUT))
    269            {
    270              if ( App_TaskID )
    271              {
    272                /*
    273                   If Application is ready to receive and there is something
    274                   in the Rx buffer then send it up
    275                */
    276                if ((MT_UartZAppRxStatus == MT_UART_ZAPP_RX_READY ) && (length != 0))
    277                {
    278                  /* Disable App flow control until it processes the current data */
    279                   MT_UartAppFlowControl (MT_UART_ZAPP_RX_NOT_READY);
    280          
    281                  /* 2 more bytes are added, 1 for CMD type, other for length */
    282                  msg_ptr = (osal_event_hdr_t *)osal_msg_allocate( length + sizeof(osal_event_hdr_t) );
    283                  if ( msg_ptr )
    284                  {
    285                    msg_ptr->event = SPI_INCOMING_ZAPP_DATA;
    286                    msg_ptr->status = length;
    287          
    288                    /* Read the data of Rx buffer */
    289                    HalUARTRead( MT_UART_DEFAULT_PORT, (uint8 *)(msg_ptr + 1), length );
    290          
    291                    /* Send the raw data to application...or where ever */
    292                    osal_msg_send( App_TaskID, (uint8 *)msg_ptr );
    293                  }
    294                }
    295              }
    296            }
